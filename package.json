{
  "name": "chrome-instant-screenshot",
  "version": "0.0.1",
  "description": "take screenshot from your browser",
  "scripts": {
    "dev": "webextension-toolbox dev chrome",
    "build": "webextension-toolbox build",
    "build:chrome": "webextension-toolbox build chrome --target chrome-instant-screenshot"
  },
  "devDependencies": {
    "webextension-toolbox": "latest"
  }
}
